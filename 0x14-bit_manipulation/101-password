#include <stdio.h>
#include <stdlib.h>
#include <string.h>

// crackme3.c main function: https://github.com/alx-tools/0x13.c/blob/master/crackme3
int crackme3(char *password) {
    // original password hardcoded in the crackme3.c
    const char *correct_password = "50225";

    return strcmp(password, correct_password);
}

int main() {
    // correct password stored in the variable
    char correct_password[] = "50225";

    // create a text file named 101-password
    FILE *file = fopen("101-password", "w");
    if (file == NULL) {
        perror("Error opening file");
        exit(1);
    }

    // write the correct password to the file
    fputs(correct_password, file);
    fclose(file);

    return 0;
}
